<p id="notice"><%= notice %></p>

<h1>Listing Disaster Pictures</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @disaster_pictures.each do |disaster_picture| %>
      <tr>
        <td><%= link_to 'Show', disaster_picture %></td>
        <td><%= link_to 'Edit', edit_disaster_picture_path(disaster_picture) %></td>
        <td><%= link_to 'Destroy', disaster_picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Disaster picture', new_disaster_picture_path %>
